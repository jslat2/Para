<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Basic Info"
        android:id="@+id/basicInfo"
        android:layout_gravity="center_horizontal"
        android:onClick="openBasicInfoEditor" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Home"
        android:id="@+id/home"
        android:layout_gravity="center_horizontal"
        android:onClick="openHome" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Billing"
        android:id="@+id/billing"
        android:layout_gravity="center_horizontal"
        android:onClick="openBilling" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Files"
        android:id="@+id/button5"
        android:layout_gravity="center_horizontal"
        android:onClick="openFileNav" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Logout"
        android:id="@+id/button6"
        android:layout_gravity="center_horizontal"
        android:onClick="logout" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="To Do"
        android:id="@+id/button7"
        android:layout_gravity="center_horizontal"
        android:onClick="openToDo" />

</LinearLayout><!-- From: file:/C:/Users/Gaming/Desktop/Para/app/src/main/res/layout/clienthome.xml -->